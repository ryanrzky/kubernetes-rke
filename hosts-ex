[all:vars]
user=nama-user
password=pass-user
ansible_ssh_extra_args='-o StrictHostKeyChecking=no'
ansible_python_interpreter=/usr/bin/python3

[master]
master1 ansible_host=# ansible_user=root ansible_ssh_port=22 ansible_ssh_private_key_file=#
master2 ansible_host=# ansible_user=root ansible_ssh_port=22 ansible_ssh_private_key_file=#

[worker]
worker1 ansible_host=# ansible_user=root ansible_ssh_port=22 ansible_ssh_private_key_file=#
worker2 ansible_host=# ansible_user=root ansible_ssh_port=22 ansible_ssh_private_key_file=#